rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Ensure users can only read/write to their own folder in the 'users' directory.
    // The path to the file must start with 'users/{userId}/'.
    match /users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
