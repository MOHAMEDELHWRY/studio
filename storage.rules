rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow reads and writes only for the authenticated user on their own files.
    // The path in the app code is `users/{currentUser.uid}/...` which matches this rule.
    match /users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
